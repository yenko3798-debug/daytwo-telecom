HTTP_PORT=4000
BRIDGE_TOKEN=replace-with-bridge-token
PANEL_BASE_URL=http://panel.local:3000
MEDIA_BASE_URL=https://panel.public.example
PANEL_ARI_TOKEN=replace-with-ari-internal-token
PANEL_WEBHOOK_URL=http://panel.local:3000/api/webhooks/ari
ARI_BASE_URL=http://127.0.0.1:8088
ARI_USERNAME=spotlight
ARI_PASSWORD=replace-with-ari-password
ARI_APPLICATION=spotlight
ASTERISK_TRANSPORT=transport-udp
ASTERISK_CONTEXT=outbound
ASTERISK_CODECS=ulaw,alaw
ASTERISK_PJSIP_DIR=/etc/asterisk/pjsip.d
ASTERISK_SOUNDS_DIR=/var/lib/asterisk/sounds/spotlight
SOUNDS_CACHE_DIR=/var/lib/asterisk/sounds/spotlight/cache
TTS_PROVIDER=pico
DEFAULT_RING_TIMEOUT=45
DEFAULT_DIAL_TIMEOUT=30
LOG_LEVEL=info
